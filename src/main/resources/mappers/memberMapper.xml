<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="MemberMapper"> <!-- 파일이름x / 예명  -->
	
	<insert id="insertMember"> 
	 	insert into member(member_num, member_id, member_pass,member_name)
	 	values(#{member_num}, #{member_id},#{member_pass}, #{member_name})
	</insert>  
	
	<select id="nuserCheck" 
                  resultType="com.itwillbs.domain.MemberDTO">
    select * from member       
    where member_nid = #{member_nid}
  	</select> 
  	
  	<select id="getMembers" resultType="map">
    SELECT * FROM member;
   </select>
  	
</mapper>
